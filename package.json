{
  "name": "hydro-web",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:poc": "ng build --configuration=poc --output-path=poc/dist/",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "stylelint \"./src/**/*.scss\" \"./src/styles.scss\"",
    "e2e": "ng e2e",
    "format": "prettier --config .prettierrc --write \"./src/**/*.ts\" \"./src/**/*.scss\" \"./src/styles.scss\"",
    "format:ci": "prettier --config .prettierrc --list-different \"./src/**/*.ts\" \"./src/**/*.scss\" \"./src/styles.scss\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/cdk": "7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/material": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "@auth0/angular-jwt": "2.0.0",
    "@types/quill": "^2.0.1",
    "@types/vertx3-eventbus-client": "3.5.0",
    "core-js": "2.5.4",
    "hammerjs": "2.0.8",
    "ngx-quill-wrapper": "7.2.0",
    "rxjs": "6.3.3",
    "vertx3-eventbus-client": "3.6.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.0",
    "@angular/cli": "7.1.1",
    "@angular/compiler-cli": "7.1.1",
    "@angular/language-service": "7.1.1",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "prettier": "1.14.3",
    "protractor": "5.4.0",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "3.1.6"
  }
}
